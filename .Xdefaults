! Extensions
URxvt.perl-ext-common:  default,matcher,font-size,tabbed
!tabbedex, resize-font, clipboard
! Keymaps
URxvt.iso14755: false
URxvt.iso14755_52: false
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste

! Clickable URLs
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! To control tabs use:
! Key 	Description
! Shift+Down 	New tab
! Shift+Left 	Go to left tab
! Shift+Right 	Go to right tab
! Ctrl+Left 	Move tab to the left
! Ctrl+Right 	Move tab to the right
! Ctrl+d 	Close tab
!
URxvt.tabbed.new-button: yes
URxvt.tabbed.tabbar-fg: 2 !foreground bar
URxvt.tabbed.tabbar-bg: 0 !background bar
URxvt.tabbed.tab-fg: 3 ! foreground tab
URxvt.tabbed.tab-bg: 0 ! background tab
!URxvt.tabbed.autohide: true
!URxvt.tabbed.reopen-on-close: yes

!URxvt.secondaryScreen: 1
!URxvt.secondaryScroll: 0
