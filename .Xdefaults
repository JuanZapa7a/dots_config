! Window
! ! Title (default urxvt)
! URxvt.title:Console
!! Scrollbar: true or false
URxvt.scrollBar: false
!! Character separation
URxvt.letterSpace: 0
!! Borders
!URxvt.borderLess: true
!URxvt.internalBorder: 0
!URxvt.externalBorder: 0
!! Icon
URxvt.iconFile:/usr/share/icons/Yaru/scalable/apps/terminal-app-symbolic.svg

! Fonts
!URxvt.font: xft:MesloLGS NF:size=11:antialias=true:hinting:true,xft:PowerlineSymbols.otf:style=Medium
URxvt.font:xft:MesloLGS NF:size=10:antialias=true:hinting=true
!,xft:MesloLGS NF:size=11,xft:FontAwesome.size=6
!URxvt.font: xft:MesloLGS NF:size=11
!URxvt.font: xft: DejaVu Sans Mono for Powerline:style=Book:size=11
!URxvt.font: xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true
!URxvt.font: xft:MesloLGS NF:size=11,xft:FontAwesome:size=11
!URxvt.font: xft:Meslo LG S:style=Regular:size=11,xft:FontAwesome:size=11 
!URxvt.font: xft:Inconsolata for Powerline:size=11 

!! Transparency
URxvt.depth: 32

!! Colour
!URxvt.background: rgba:9200/6500/9a00/ee00
!URxvt.background: rgb:1111/1111/1111
!URxvt.foreground: rgb:baba/bdbd/b6b6
!URxvt.background: rgb:48/10/36
!URxvt.foreground: 7

